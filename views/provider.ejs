<html>
    <head>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/css/materialize.min.css">
        <link rel='stylesheet' href='/stylesheets/style.css'/>
        <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
        <link href='http://fonts.googleapis.com/css?family=Merienda+One' rel='stylesheet' type='text/css'>
    </head>
    
    <body>
        
         <%# Header %>
        <div id="top-rectangle"></div>
        <div id="horizontal-line"></div>
        <div id="name">Min Hoo Lee</div>
        <div id="page-name">Povider Info</div>
        <div id="company-name"><b>Angel Care</b></div>
        
        <%# Sidebar Content %>
        <div id="left-rectangle"></div>
        <div id="vertical-line"></div>
        
        <div class="dashboard">
            <img src="/images/dashboard_icon.png" alt="Dashboard" class="dashboard-icon">
            <form action="/" method="GET">
                <input type="submit" value="Dashboard" id="dashboard-button">
            </form>
        </div>
        
         <div class="insurance">
            <img src="/images/insurance.png" alt="Insurance" class="insurance-icon">
            <form action="/" method="GET">
                <input type="submit" value="Insurance" id="insurance-button">
            </form>
        </div>
        
          <div class="service-pricing">
			<img src="/images/services_prices_icon.png" alt="Health Service Pricing" class="service-icon">
            <form action="/cpt" method="POST">
                <input type="submit" value="Service Pricing" id="costs-button">
            </form>
        </div>
        <style>
            #bar-graph-rectangle
        </style>
        <div id="bar-graph-rectangle">
            <h5>Name</h5>
            <h6><%="Dr." + JSON.parse(JSON.stringify(pfirstName)) +  "   " + JSON.parse(JSON.stringify(plastName))%><br></h6>
            <h5>Degree</h5>
            <h6><%=JSON.parse(JSON.stringify(degree))%><br></h6>
            <h5> Fax</h5>
            <h6><%=JSON.parse(JSON.stringify(fax))%><br></h6>
            <h5>Phone</h5>
            <h6><%=JSON.parse(JSON.stringify(phone))%><br></h6>
            <h5> Primary Specialty</h5>
            <h6><%=JSON.parse(JSON.stringify(specialty_primary))%><br></h6>
            <h5>Board Certifications </h5>
            
            <h6><%=JSON.parse(JSON.stringify(board_certifications))%><br></h6>
            
            
            <h2><%="Dr." + JSON.parse(JSON.stringify(pfirstName)) +  "   " + JSON.parse(JSON.stringify(plastName))%><br></h2>
            <img src="/images/doctor.png" alt="Mountain View" style="width:304px;height:228px;">
        </div>
        
       


    
    </body>
    
</html>
