
<html>
    <head>
        <link rel='stylesheet' href='/stylesheets/style.css'/>
        <link href='https://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
        <link href='http://fonts.googleapis.com/css?family=Merienda+One' rel='stylesheet' type='text/css'>
    </head>
    <body>
        <%# Header %>
        <div id="top-rectangle"></div>
        <div id="left-rectangle"></div>
        <div id="vertical-line"></div>
        <div id="horizontal-line"></div>
        <img src="/images/user_icon.png" alt="Profile" class="profile"> 
        <div id="name">Min Hoo Lee</div>
        <div id="company-name"><b>Angel Care</b></div>
        <h1> <%= JSON.stringify(desc)%></h1>
        <div id="chart"></div>
        <script src="http://d3js.org/d3.v3.min.js"  charset="utf-8"></script>
        <script>
            var data = [<%=JSON.stringify(low)%>, <%=JSON.stringify(average)%>, <%=JSON.stringify(highPrice)%>];
            var x = d3.scale.linear()
                .domain([0, d3.max(data)])
                .range([0, 420]);

             d3.select("#chart")
                .selectAll("div")
                .data(data)
                .enter().append("div")
                .style("width", function(d) { return x(d) + "px"; })
                .text(function(d) { return d; });
        </script>
        <form action='/cpt' method='POST'>
            <input type="text" name="code"><br>
            <input type="submit" value="Submit">
        </form>
        <script>document.write('<script src="http://' + (location.host || 'localhost').split(':')[0] + ':35729/livereload.js?snipver=1"></' + 'script>')</script>
    </body>
</html>
